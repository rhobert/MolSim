<?xml version="1.0"?>

<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://www5.in.tum.de/wiki/index.php/PSE_Molekulardynamik_WS12"
xsi:schemaLocation="http://www5.in.tum.de/wiki/index.php/PSE_Molekulardynamik_WS12 src/input/InputParameters.xsd">
		
	<outputPhaseSpace>sheet5/phaseSpaceMeasurement.txt</outputPhaseSpace>
	
	<t_end>5</t_end>
	
	<delta_t>0.0005</delta_t>
	
	<potential>
		<lennardJones />
	</potential>
	
	<gravitation>
		<x>0</x>
		<y>-9.81</y>
		<z>0</z>
	</gravitation>
	
	<dimensionCount>3</dimensionCount>

	<domain>		
		<dimensions>
			<x>26</x>
			<y>26</y>
			<z>26</z>
		</dimensions>
		
		<x>
			<lower>periodic</lower>
			<upper>periodic</upper>
		</x>
		<y>
			<lower>reflecting</lower>
			<upper>reflecting</upper>
		</y>
		<z>
			<lower>periodic</lower>
			<upper>periodic</upper>
		</z>
		
		<cutoff>2.6</cutoff>	
	</domain>
	
	<thermostat>	
		<initialT>10</initialT>
		<frequency>50</frequency>
	</thermostat>
	
	<inputs>		
		<cuboid>			
			<position>
				<x>0.5</x>
				<y>1</y>
				<z>0.5</z>
			</position>
			
			<velocity>
				<x>0</x>
				<y>0</y>
				<z>0</z>
			</velocity>
			
			<dimensions>
				<x>25</x>
				<y>25</y>
				<z>25</z>
			</dimensions>
			
			<mass>1.0</mass>
			
			<distance>1.0</distance>
			
			<sigma>0.8909</sigma>
			
			<epsilon>1.0</epsilon>
			
			<type>0</type>
		</cuboid>
		
	</inputs>
	
</simulation>
