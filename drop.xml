<?xml version="1.0"?>

<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://www5.in.tum.de/wiki/index.php/PSE_Molekulardynamik_WS12"
xsi:schemaLocation="http://www5.in.tum.de/wiki/index.php/PSE_Molekulardynamik_WS12 src/input/InputParameters.xsd">
	<outputFile>drop_vtk</outputFile>
	<inputs>
		<cuboid>
			<position>
				<x>1.1225</x>
				<y>1.1225</y>
				<z>0</z>
			</position>
			<velocity>
				<x>0</x>
				<y>0</y>
				<z>0</z>
			</velocity>
			<dimensions>
				<x>100</x>
				<y>20</y>
				<z>1</z>
			</dimensions>
			<mass>1</mass>
			<distance>1.1225</distance>
		</cuboid>
		<sphere>
			<position>
				<x>57.25</x>
				<y>50</y>
				<z>0</z>
			</position>
			<velocity>
				<x>0</x>
				<y>-100</y>
				<z>0</z>
			</velocity>
			<dimensionCount>2</dimensionCount>
			<radiusDimension>15</radiusDimension>
			<mass>1</mass>
			<distance>1.1225</distance>
		</sphere>
	</inputs>
	<writeFrequency>10</writeFrequency>
	<t_end>2.0</t_end>
	<delta_t>0.0001</delta_t>
	<potential>lenard jones</potential>
	<domain boundary="reflecting" cutoff="3">
		<x>115</x>
		<y>115</y>
		<z>0</z>
	</domain>
	<brownianMotion>
		<dimensionCount>2</dimensionCount>
		<meanVelocity>0.1</meanVelocity>
	</brownianMotion>
</simulation>